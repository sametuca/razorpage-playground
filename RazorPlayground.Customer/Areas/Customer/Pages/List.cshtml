@page
@model RazorPlayground.Customer.Areas.Customer.Pages.ListModel
@{
}

<table class="table table-striped table-dark">
    <thead>
        <tr>
            <th scope="col">İsim</th>
            <th scope="col">Soyisim</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Customers)
        {
            <tr>
                <th scope="row">1</th>
                <td>@item.Id</td>
                <td>@item.Name</td>
            </tr>
        }
  
    </tbody>
</table>